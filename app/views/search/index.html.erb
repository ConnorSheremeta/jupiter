<% page_title( t('.header')) %>
<% content_for :head do %>
  <meta name="robots" content="noindex" />
<% end %>
<div class="container mt-3">
  <ul class="nav nav-tabs">
    <% [:item, :collection, :community].each do |model| %>
      <%= results_model_tab_link(model) %>
    <% end %>
  </ul>

  <%= render partial: 'results', locals: { results: @results[@active_tab] } %>
</div>
